<div class="input">
  <form action="/getJoke" method="get">
    <label for="name">What is your Name? : </label>
    <input type="text" name="name" required />
    <input type="submit" value="Send Name" />
  </form>
</div>

<div class="output">
  <% if (typeof result !== 'undefined' && result !== null) { %>
  <div class="joke">
    <p><%= result.setup %></p>
    <p><%= result.delivery %></p>
  </div>

  <% } %>
</div>
